{
  "name": "pdfme-graphic-schema",
  "version": "1.0.0-alpha.2",
  "description": "A TypeScript package supporting both browser and Node.js environments",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/necessarylion/pdfme-graphic-schema.git"
  },
  "bugs": {
    "url": "https://github.com/necessarylion/pdfme-graphic-schema/issues"
  },
  "homepage": "https://github.com/necessarylion/pdfme-graphic-schema#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": {
    "name": "necessarylion",
    "url": "https://github.com/necessarylion"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^19.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "csstype": "^3.1.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@10.6.3",
  "dependencies": {
    "@pdfme/common": "^5.2.5",
    "@pdfme/pdf-lib": "^1.18.3",
    "fast-xml-parser": "^5.2.3",
    "lucide": "^0.510.0"
  }
}